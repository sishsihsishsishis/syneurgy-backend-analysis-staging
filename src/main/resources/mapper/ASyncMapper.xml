<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aws.sync.mapper.ASyncMapper">
    <insert id="addBatch" parameterType="com.aws.sync.entity.Async">
        insert into a_sync_table (meeting_id, start_time,a_sync)
        values
        <foreach collection="Async" item="a" separator=",">
            (#{a.meeting_id}, #{a.start_time},#{a.a_sync})
        </foreach>
    </insert>

    <select id="findSync" parameterType="java.lang.Long" resultType="com.aws.sync.vo.csv.SyncA">
        select start_time,a_sync from a_sync_table where meeting_id = #{param1} order by start_time ASC
    </select>


</mapper>